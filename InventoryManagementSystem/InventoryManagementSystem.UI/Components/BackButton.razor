<MudButton StartIcon="@Icons.Material.Filled.ArrowBack" Color="Color.Primary" Variant="Variant.Filled" Class="mb-2" OnClick="GoBack">
    Back
</MudButton>

@code {
    [Inject]
    public IJSRuntime JSRuntime { get; set; }
    private async Task GoBack()
    {
        await JSRuntime.InvokeVoidAsync("history.back");
    }
}
